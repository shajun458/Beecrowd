#include<stdio.h>
int main ()
{
    double a, b, c, pi, tri, cir, tra, sqr,rec;
    scanf("%lf %lf %lf",&a,&b,&c);
    pi=3.14159;
    tri = .5 *a*c;
    cir = pi*c*c;
    tra = (a+b)/2*c;
    sqr = b*b;
    rec = a*b;
    printf("TRIANGULO: %.3lf\n",tri);
    printf("CIRCULO: %.3lf\n",cir);
    printf("TRAPEZIO: %.3lf\n",tra);
    printf("QUADRADO: %.3lf\n",sqr);
    printf("RETANGULO: %.3lf\n",rec);
    return 0;
}
