#include<stdio.h>

int main()
{
    int x,u;
    float p=0;
    scanf("%d %d",&x,&u);
    if (x==1)
    {

        p= (float)(4.00*u);
    }
    else if (x==2)
    {
        p=(float)(4.50*u);
    }
    else if(x==3)
    {
        p=(float) (5.00*u);
    }
    else if(x==4)
    {
        p=(float)(2.00*u);
    }
    else if(x==5)
    {
        p=(float)(1.50*u);
    }
    printf("Total: R$ %.2f\n",p);

    return 0;
}
